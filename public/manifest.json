{
  "manifest_version": 3,
  "name": "My Extension",
  "version": "1.0",
  "description": "This extension is a starting point to create a real extension.",
  "permissions": ["tabs","sidePanel", "scripting", "activeTab", "identity","https://docs.google.com/*"],
  "host_permissions": [
    "https://docs.google.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "side_panel": {
    "default_path": "index.html"
  }
}
